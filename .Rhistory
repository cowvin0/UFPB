select(- Rings)
data |>
vis_dat()
Frequência <- data |>
ggplot(aes(x = forcats::fct_infreq(Sex))) +
geom_bar(
aes(y = after_stat(count) / sum(after_stat(count)), fill = Sex),
show.legend = FALSE,
) +
theme_bw() +
theme(axis.title.x = element_blank()) +
scale_fill_manual(
values = viridis::mako(5)[3:5]
) +
labs(y = NULL, x = NULL)
Frequência
Distribuição <- data |>
pivot_longer(
!Sex,
names_to = "variable",
values_to = "values"
) |>
ggplot(
aes(
x = values,
fill = Sex
)
) +
geom_violin(
aes(y = variable),
draw_quantiles = c(0.25, 0.5, 0.75),
) +
labs(x = "", y = "") +
facet_wrap(vars(variable), scales = "free") +
scale_fill_manual(
values = viridis::mako(5)[3:5],
name = "Sexo"
) +
theme_bw() +
theme(axis.text.y = element_blank())
Distribuição
data <- data |>
(\(x) {
scaled <- x |>
select_if(is.numeric) |>
scale() |>
as_tibble()
x |>
select_if(is.character) |>
cbind(scaled)
})()
data |>
select_if(is.numeric) |>
cor() |>
corrplot(
method="circle",
col = mako(200),
diag = FALSE,
addgrid.col = "#00000040",
tl.pos = "l",
tl.cex = 0.6,
tl.col = "black",
tl.offset = 0.25,
cl.length = 9,
cl.cex = 0.6,
cl.ratio = 0.25,
family = "mono"
)
data |>
select_if(is.numeric) |>
cor() |>
corrplot(
method="circle",
col = mako(200),
diag = FALSE,
addgrid.col = "#00000040",
tl.pos = "l",
tl.cex = 0.6,
tl.col = "black",
tl.offset = 0.25,
cl.length = 9,
cl.cex = 0.6,
cl.ratio = 0.25,
family = "mono"
)
Componentes <- prcomp(
data |>
select_if(is.numeric),
center = TRUE, tol=0
)
Var <- fviz_eig(
Componentes,
addlabels = TRUE,
barfill="#002951"
) +
labs(
y = "Porcentagem da variância explicada",
x = "Dimensões",
title = ""
)
Var
Contrib <- fviz_cos2(
Componentes,
choice = "var",
axes = 1:2,
fill="#002951"
) +
labs(
y = "Representação",
title = ""
)
Contrib
Vec <- fviz_pca_var(
Componentes,
col.var = "cos2",
gradient.cols = c("#002951", "#eeeeee","#a0000a"),
repel = TRUE
) +
labs(title = "")
Vec
